<%- include('header') -%>
<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>
<div class="box">
  <% items.forEach((ele) => { %>
  <div class="item">
    <input type="checkbox" />
    <p><%= ele.name %></p>
  </div>
  <% }) %>

  <form class="item" action="/" method="POST">
    <input type="text" placeholder="New Item" name="todo" autocomplete="off" />
    <button type="submit" name="list" value="<%=listTitle%>">+</button>
  </form>
</div>

<%- include('footer') -%>
